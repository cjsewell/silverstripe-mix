{
    "name": "ssmix/mix",
    "description": "Laravel Mix support for Silverstripe.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "laravel Mix"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.0",
        "ext-json": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "SSMix\\": "src/",
            "SilverStripeModule\\Skeleton\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
